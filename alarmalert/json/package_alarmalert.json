{
  "zip": {
    "bucketNamePostfix": ".sgas.cto.lambda-files",
    "keyName": "nodejs/alarmalert.zip",
    "zipFile" : "alarmalert.zip",
    "sourceFolder" : "/Users/alex.ough/Projects/Node/aws-services",
    "src" : ["alarmalert/*.js", "alarmalert/json/*", "alarmalert/node_modules/**/*", "lib/**/*"]
  },
  "cron": {
    "queueName": "CTOCronQueue",
    "messageBody": "cto-cron-message",

    "functionName": "cto-cron-runner",
    "handler":"alarmalert/index_cron.handler",
    "memorySize": 128,
    "timeout": 30
  },
  "alertEmail": {
    "labelId": "INBOX",
    "userId": "me",
    "eventLogGroupName": "alert_email_group",
    "alarmName": "AlertEmailAlarm",
    "topicName": "AlertEmailTopic",
    "subscriberEmail": "alex.ough@sungardas.com",
    "functionName": "email-to-eventlog",
    "handler":"alarmalert/index_eventlog.handler",
    "memorySize": 128,
    "timeout": 60
  },
  "lambda": {
    "roleName": "lambda_alarmalert_invoke",
    "assumeRolePolicyName": "lambda_assume_role_policy",
    "inlinePolicyName": "lambda_alarmalert_execution_policy"
  }
}

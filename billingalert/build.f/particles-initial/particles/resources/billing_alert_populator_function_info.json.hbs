{{#resource}}
  "Type": "Custom::BillingAlertPopulatorFunctionInfo",
  "Properties": {
    "ServiceToken": { "Fn::GetAtt" : ["DeployerLambdaFunction", "Arn"] },
    "AWSAccountId": { "Ref": "AWS::AccountId" },
    "PackageJSON": {
      "region": { "Ref": "AWS::Region" },
      "bucketName": { "Fn::Join" : [ "", [ { "Ref" : "AWS::AccountId" }, { "Ref": "DeployerS3BucketPostfix" }, { "Ref" : "AWS::Region" } ]]},
      "keyName": "nodejs/billingalert_populator.zip",
      "functionName": "billingalert_populator",
      "handler":"billingalert/index_populator.handler",
      "memorySize": 128,
      "timeout": 30
    },
    "RoleArn": { "Fn::GetAtt" : [ "BillingAlertPopulatorFunctionRoleInfo", "Arn" ]}
  }
{{/resource}}

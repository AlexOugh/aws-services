{{#resource}}
"Type": "AWS::Lambda::Function",
"Properties": {
  "FunctionName": "awsconfig-checker",
  "Code": {
    "S3Bucket": "{{s3.bucket}}",
    "S3Key": "{{assetPath 'awsconfig.zip'}}"
  },
  "Handler": "awsconfig/index_checker.handler",
  "Runtime": "nodejs",
  "Timeout": "60",
  "MemorySize": "128",
  "Role": { "Fn::GetAtt" : ["AWSConfigFunctionRoleInfo", "Arn"] }
}
{{/resource}}
